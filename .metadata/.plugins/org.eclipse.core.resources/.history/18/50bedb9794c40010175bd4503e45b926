package com.repository;

import java.util.Optional;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.*;
import com.model.UserLoginRequest;
@Repository
public interface UserRepository extends JpaRepository<UserLoginRequest, Long> {
	public Optional<?> findBymobileNo(String no);
	public Optional<UserLoginRequest> findByUserid(String uname);
	
}
